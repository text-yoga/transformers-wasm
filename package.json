{
  "name": "@text-yoga/transformers-wasm",
  "version": "1.0.0",
  "description": "⚠️ **WARNING**: This is very early stage, use at your own risk ⚠️",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "wasm-pack build -s text-yoga --dev --target web",
    "build:release": "wasm-pack build -s text-yoga --release --target web",
    "test:server": "npx http-server --cors -p 31300  ./tests/data",
    "test:chrome": "wasm-pack -vvv test --chrome --chromedriver \"$(which chromedriver)\"",
    "test:firefox": "wasm-pack test --firefox --geckodriver \"$(which geckodriver)\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1"
  }
}
